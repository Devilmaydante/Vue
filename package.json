{
  "name": "waosVue",
  "description": "Node - Boilerplate Back : Express, Jwt, Mongo, Sequelize (Beta)  (Based on meanjs and riess.js)",
  "version": "0.1.0",
  "keywords": [
    "vue",
    "vuex",
    "vuetify",
    "jwt",
    "jest",
    "stack",
    "boilerplate"
  ],
  "private": false,
  "author": "https://github.com/weareopensource/Vue/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weareopensource/Vue.git"
  },
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.11.3"
  },
  "scripts": {
    "start": "run-s generateConfig vue:serve",
    "build": "run-s generateConfig vue:build",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "commit": "npx git-cz",
    "release": "standard-version",
    "generateConfig": "node ./scripts/generateConfig.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "lodash": "^4.17.15",
    "snyk": "^1.235.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.4",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^4.0.0-rc.8",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0-rc.8",
    "@vue/cli-plugin-eslint": "^4.0.0-rc.8",
    "@vue/cli-plugin-router": "^4.0.3",
    "@vue/cli-plugin-unit-jest": "^4.0.3",
    "@vue/cli-plugin-vuex": "^4.0.3",
    "@vue/cli-service": "^4.0.0-rc.8",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2",
    "commitizen": "^4.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.9",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.4",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "standard-version": "^7.0.0",
    "vue-cli-plugin-vuetify": "^1.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0",
    "waos-conventional-changelog": "^1.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/waos-conventional-changelog"
    }
  },
  "snyk": true
}
